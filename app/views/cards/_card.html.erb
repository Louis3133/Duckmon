<div class="card col-lg-3" id="<%= dom_id card %>" >

  <img class="card-img-top " src="<%= card.image_card %>" alt="illustration de la carter"  height="160px" >

  <p>
    <strong>Pv:</strong>
    <%= card.pv %>
  </p>

  <p>
    <strong>Name:</strong>
    <%= card.name %>
  </p>

  <p>
    <strong>Description:</strong>
    <%= card.description %>
  </p>

  <p>
    <strong>Damages:</strong>
    <%= card.damages %>
  </p>




  <p>
    <strong>Action:</strong>
    <%= card.action %>
  </p>

  <p>
    <strong>Attack name:</strong>
    <%= card.attack_name %>
  </p>

</div>
